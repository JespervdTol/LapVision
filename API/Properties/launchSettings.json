{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "LapVision.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7234;http://localhost:5082",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HTTPS_PORT": "7234",
        "ASPNETCORE_HTTP_PORT": "5082"
      },
      "dotnetRunMessages": true
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "7234",
        "ASPNETCORE_HTTP_PORTS": "5082"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}